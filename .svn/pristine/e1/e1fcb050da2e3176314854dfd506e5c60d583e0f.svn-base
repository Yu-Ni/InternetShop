#mapper {
	width: 80%;
	margin: 0 auto;
}

#header {
	height: 50px;
	opacity: 0.8;
}

#Navigation li {
	float: left;
	margin-left: 100px;
	list-style: none;
}

#search {
	margin-top: 10px;
	float: left;
	margin-left: 100px;
}

#search .btn {
	background-color: rgb(254, 112, 62);
}

#SearchHeader {
	height: 100px;
	margin-top: 30px;
}

#SearchHeader img {
	margin-left: 100px;
	float: left;
}

#SecondNav li {
	float: left;
	margin-left: 50px;
	list-style: none;
}

#SecondNav {
	width: 100%;
}

#myCarousel {
	width: 70%;
	float: left;
	height: 350px;
	opacity: 0.9;
}

.carousel {
	height: 350px;
	background-color: #000;
}

.carousel .item {
	height: 350px;
	background-color: #000;
}

.carousel img {
	height: 100%;
}

.carousel-caption {
	margin-bottom: 20px;
	line-height: 1.8;
	font-family: "Î¢ÈíÑÅºÚ";
}

#notice {
	float: left;
	width: 30%;
	height: 350px;
	border: 5px solid rgb(254, 112, 62);
	overflow: auto;
}

.panel {
	width: 100%;
	overflow: scroll;
}

.panel-heading {
	background-color: rgb(254, 112, 62);
}

.panel-body {
	height: 300px;
}

.panel-footer {
	border-bottom-color: #fff !important;
	background-color: #fff !important;
}

.bootstrap-table .table thead>tr>th {
	background-color: rgb(254, 112, 62) !important;
	color: #fff;
}

#tbNotice thead>tr {
	height: 10px !important;
}

#nav {
	width: 100%;
	height: 55px;
	border-bottom: 5px solid rgb(254, 112, 62);
}

#nav>ul {
	margin: 0;
	padding: 0;
}

#nav>ul>li {
	list-style-type: none;
	display: inline;
	float: left;
	height: 30px;
	line-height: 30px;
}

#nav>ul>li>a {
	display: block;
	width: 154px;
	color: black;
	font-size: 17px;
	text-decoration: none;
	text-align: center;
	padding: 10px;
}

#nav>ul>li>a:hover {
	background-color: #FF5902;
	color: white;
}

#tbNotice {
	font-size: 11px;
}

.modal-header {
	background-color: #FF5902 !important;
	color: white !important;
	text-align: center;
}

#footer {
	width: 100%;
	height: 100px;
	padding:20px;
	line-height:26px;
	background-color:#EAEAEA;
	clear: both;
	font-size:12px;
}

.footer_content{
	text-align:center;
}

.content_div2{
	color:#999C9E;
}

#mainTab {
	margin-bottom: 100px;
	height: auto;
	clear: both;
}